.axis path,
.axis line
    fill none
    stroke #000
    shape-rendering crispEdges

.dot
    stroke #000

 .tooltip
    position absolute
    text-align center
    width 60px
    height 28px
    padding 2px
    font 12px sans-serif
    background-color grey
    border 0px
    border-radius 8px
    pointer-events none